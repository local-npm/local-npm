{
  "name": "local-npm",
  "version": "1.6.0",
  "description": "Local and offline-first npm mirror",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "test": "echo \"There is no 'npm test'! Run ./test.sh instead.\" && exit 1",
    "run-test": "run-scripts main-test test-cleanup",
    "lint": "eslint lib/ test/",
    "write-npmrc": "echo registry=http://127.0.0.1:3030/ > .npmrc",
    "main-test": "babel-node node_modules/.bin/_mocha --grep=$GREP test/test.js",
    "test-cleanup": "rm -f .npmrc",
    "start": "node ./lib/bin.js"
  },
  "engines" : {
    "node" : ">= 4"
  },
  "author": {
    "name": "Nolan Lawson",
    "email": "nolan@nolanlawson.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/local-npm/local-npm.git"
  },
  "contributors": [
    "Gabriel J. Csapo <gabecsapo@gmail.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^2.2.2",
    "colors": "^0.6.2",
    "compression": "^1.0.10",
    "corser": "^2.0.0",
    "express": "^4.10.4",
    "express-http-proxy": "^1.0.3",
    "express-pouchdb": "^1.0.0",
    "level": "^1.3.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.2.2",
    "pouchdb": "^5.0.0",
    "request": "^2.39.0",
    "semver": "^5.0.1",
    "serve-favicon": "^2.0.1",
    "through2": "^2.0.0",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-transform-regenerator": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "child-process-promise": "^1.1.0",
    "denodeify": "^1.2.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "istanbul-coveralls": "^1.0.3",
    "memdown": "^1.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "ncp": "^2.0.0",
    "nock": "^5.2.1",
    "node-fetch": "^1.3.3",
    "rimraf": "^2.5.0",
    "run-scripts": "^0.4.0"
  }
}
